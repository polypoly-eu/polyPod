  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/poly-api.git"
  },
  "author": "Lars Hupel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/polypoly-eu/poly-api/issues"
  },
  "homepage": "https://github.com/polypoly-eu/poly-api#readme",
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "devDependencies": {
    "@polypoly-eu/eslint-config-polypoly": "0.0.4",
    "@polypoly-eu/rdf-spec": "^0.3.1",
    "@rdfjs/dataset": "^1.0.1",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@types/node-fetch": "^2.5.7",
    "@types/rdfjs__dataset": "^1.0.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chokidar-cli": "^2.1.0",
    "eslint": "^7.13.0",
    "fast-check": "^2.6.1",
    "jest": "^26.6.3",
    "memfs": "^3.2.0",
    "mocha": "^8.2.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "rollup": "^2.33.1",
    "ts-jest": "^26.4.4",
    "ts-mocha": "^8.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "@polypoly-eu/rdf-spec": "^0.3.1",
    "chai": "^4.2.0",
