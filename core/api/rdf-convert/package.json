{
  "name": "@polypoly-eu/rdf-convert",
  "version": "0.1.0",
  "description": "Converts terms and quads between different RDFJS-compliant data factories",
  "files": [
    "dist/*.ts",
    "dist/*.js"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/polyPod.git"
  },
  "scripts": {
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "lint": "npm run eslint",
    "test": "jest",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "dependencies": {
    "@types/rdf-js": "^4.0.0"
  },
  "devDependencies": {
    "@graphy/core.data.factory": "^4.3.3",
    "@graphy/memory.dataset.fast": "^4.3.3",
    "@polypoly-eu/eslint-config": "file:../../../eslint-config",
    "@polypoly-eu/rdf-spec": "file:../rdf-spec",
    "@rdfjs/data-model": "^1.2.0",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@types/jest": "^26.0.20",
    "@types/n3": "^1.4.4",
    "@types/rdfjs__dataset": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "chokidar-cli": "^2.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fast-check": "^2.11.0",
    "jest": "^26.6.3",
    "n3": "^1.8.0",
    "prettier": "^2.2.1",
    "rollup": "^2.37.0",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  },
  "private": true
}
