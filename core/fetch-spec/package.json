{
  "name": "@polypoly-eu/fetch-spec",
  "version": "0.0.1",
  "description": "Standalone executable spec for window.fetch",
  "files": [
    "dist/*.ts",
    "dist/*.js"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/polyPod.git"
  },
  "scripts": {
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "lint": "npm run eslint",
    "pretest": "start-dummy-server &",
    "test": "npm-run-all test:jest test:mocha test:jasmine test:electron",
    "test:electron": "electron-mocha --renderer --require ts-node/register src/tests/browser/*.test.ts",
    "test:jest": "jest",
    "test:mocha": "ts-mocha src/tests/node/*.test.ts",
    "test:jasmine": "ts-node node_modules/.bin/jasmine --config=jasmine.json",
    "posttest": "stop-dummy-server",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "devDependencies": {
    "@polypoly-eu/dummy-server": "file:../dummy-server",
    "@polypoly-eu/eslint-config": "file:../../eslint-config",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/jest": "^26.0.20",
    "@types/jsdom": "^16.2.6",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chokidar-cli": "^2.1.0",
    "electron": "^11.2.0",
    "electron-mocha": "^10.0.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jasmine": "^3.6.4",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "mocha": "^8.2.1",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rollup": "^2.37.0",
    "ts-jest": "^25.5.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1"
  },
  "private": true
}
