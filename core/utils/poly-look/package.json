{
  "name": "@polypoly-eu/poly-look",
  "version": "0.0.0",
  "description": "polyLook web components following open-wc recommendations",
  "author": "poly-look",
  "license": "MIT",
  "main": "src/poly-look.js",
  "module": "src/poly-look.js",
  "scripts": {
    "lint": "eslint --ext .js,.html . --ignore-path .gitignore && prettier \"**/*.js\" --check --ignore-path .gitignore",
    "format": "eslint --ext .js,.html . --fix --ignore-path .gitignore && prettier \"**/*.js\" --write --ignore-path .gitignore",
    "test": "web-test-runner --coverage",
    "test:watch": "web-test-runner --watch",
    "storybook": "web-dev-server -c .storybook/server.mjs",
    "build": "rollup -c rollup.config.js"
  },
  "dependencies": {
    "d3": "^7.0.4",
    "lit-element": "^2.5.1",
    "lit-html": "^1.4.1",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.5.33",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-sucrase": "^4.0.1",
    "@web/dev-server": "^0.1.22",
    "@web/dev-server-rollup": "^0.3.5",
    "@web/dev-server-storybook": "^0.0.2",
    "@web/test-runner": "^0.13.16",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^7.2.0",
    "identity-obj-proxy": "^3.0.0",
    "prettier": "^2.2.1",
    "rollup": "^2.48.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
