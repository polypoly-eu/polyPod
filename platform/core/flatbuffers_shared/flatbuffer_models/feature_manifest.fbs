table Link {
    name:string (required);
    url:string (required);
}

table FeatureManifest {
    name:string;
    author:string;
    version:string;
    description:string;
    thumbnail:string;
    thumbnail_color:string;
    primary_color:string;
    links:[Link];
    border_color:string;
}

root_type FeatureManifest;